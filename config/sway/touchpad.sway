# vim:ft=swayconfig:

input type:touchpad {
  events enabled
  accel_profile adaptive
  pointer_accel 0.45
  natural_scroll enabled
  scroll_factor 0.8
  scroll_method two_finger
  left_handed disabled
  tap enabled
  tap_button_map lrm
  drag enabled
  drag_lock disabled
  dwt enabled
  dwtp enabled
  rotation_angle 0.0
  middle_emulation enabled
  click_method button_areas
}
