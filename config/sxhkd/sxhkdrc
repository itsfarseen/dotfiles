Print
  flameshot gui

XF86AudioLowerVolume
  pactl set-sink-volume @DEFAULT_SINK@ -3dB

XF86AudioRaiseVolume
  pactl set-sink-volume @DEFAULT_SINK@ +3dB

super + F3
  pavucontrol

super + Return
  alacritty

super + d
  rofi -show drun -show-icons

super + e
  pcmanfm

super + p
  $HOME/.config/sxhkd/cycle-monitor; sleep 1s; i3-msg restart; sleep 0.5s; systemctl --user restart polybar; 

super + s
  flameshot gui

super + w
  firefox

super + {Escape,F1,F2}
  xbacklight {-set 0.06,-5,+5}

